<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cocoa.dao.ProductDao">
	<select id="selectById" resultType="com.cocoa.model.PmsProduct">
		SELECT * 
		FROM pms_product
		WHERE id=#{id};
	</select>
</mapper>